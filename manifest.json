{
	"name": "Pomodoro Timer",
	"version": "1.0",
	"description": "A simple Pomodoro Timer to help manage your productivity.", 
	"manifest_version": 3,
	"action": {
		"default_popup": "popup.html",
		"default_title": ""
	},
	"background": {
		"service_worker": "background.js"
	},
	"permissions": ["notifications", "storage"]
}

